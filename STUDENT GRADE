#include<stdio.h>
int main()
{
	float maths,phy,total,avg;
	printf("maths marks \n",maths);
	printf("physics marks\n",phy);
	scanf("%f%f",&phy,&maths);
	total=maths+phy;
	avg=total/2;
	printf("percentage is %.2f\n",avg);
	if (avg>=90)
	printf("A grade\n");
	else if(avg>=80 && avg<90)
	printf("B grade\n");
	else if(avg>=70 && avg<80)
	printf("c grade\n");
	else if(avg>=60 && avg<70)
	printf(" D grade\n");
	else if(avg>=50 && avg<60)
	printf("E grade\n");
	else
	printf("fail");
	
	
	
}
